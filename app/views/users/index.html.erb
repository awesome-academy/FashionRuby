<% provide(:title, 'Users') %>
<table class="table table-hover">

    <thead>
        <tr>
            <th scope="col">
                <%=t("global.canpaigs.name") %>
            </th>
        </tr>
    </thead>
    <tbody>
        <% @users.each do |user| %>
        <tr>
            <td>
                <%= link_to user.name ,current_user %>
            </td>
        </tr>
        <% end %>
    </tbody>
</table>
<%= will_paginate %>
